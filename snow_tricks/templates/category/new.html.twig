{% extends 'base.html.twig' %}

{% block title %}Créer une catégorie{% endblock %}

{% block body %}
    <h1>Créer une catégorie</h1>

    {{ form_start(categoryType) }}
        {{ form_row(categoryType.name)}}

        <button class="btn btn-primary" type="submit">Créer</button>
        <a class="btn btn-outline-danger" href="{{ path('app_category') }}">Retour</a>
    {{ form_end(categoryType)}}
{% endblock %}
